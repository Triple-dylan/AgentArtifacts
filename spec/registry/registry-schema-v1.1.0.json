{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://agentassets.io/spec/registry/registry-schema-v1.1.0.json",
  "title": "Agent Assets Registry Item",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "registry_schema_version",
    "id",
    "type",
    "name",
    "description",
    "version",
    "format",
    "files",
    "size_bytes",
    "compatibility",
    "license",
    "price_usd",
    "currency",
    "billing_type",
    "availability",
    "created_at",
    "updated_at",
    "urls",
    "support"
  ],
  "properties": {
    "registry_schema_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
    },
    "id": {
      "type": "string",
      "pattern": "^AA-[A-Z]{3}-[A-Z0-9-]+-[0-9]{3}$"
    },
    "type": {
      "type": "string",
      "enum": ["prompt", "skill", "agent", "utility", "doc", "bundle"]
    },
    "name": { "type": "string", "minLength": 1 },
    "description": { "type": "string", "minLength": 1 },
    "version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$"
    },
    "format": {
      "type": "string",
      "enum": ["md", "json", "yaml", "code", "mixed", "md;json", "md;yaml", "yaml;json", "md;json;yaml"]
    },
    "files": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["path", "type", "bytes"],
        "properties": {
          "path": { "type": "string" },
          "type": { "type": "string" },
          "bytes": { "type": "integer", "minimum": 0 }
        }
      }
    },
    "checksum": { "type": "string" },
    "size_bytes": { "type": "integer", "minimum": 0 },
    "input_schema": {
      "type": ["object", "string"]
    },
    "output_schema": {
      "type": ["object", "string"]
    },
    "compatibility": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "dependencies": {
      "type": "array",
      "items": { "type": "string" }
    },
    "required_secrets": {
      "type": "array",
      "items": { "type": "string" }
    },
    "license": {
      "type": "object",
      "additionalProperties": false,
      "required": ["type", "seats", "redistribution_policy", "commercial_use"],
      "properties": {
        "type": {
          "type": "string",
          "enum": ["personal", "commercial", "seat_based", "custom"]
        },
        "seats": {
          "oneOf": [
            { "type": "integer", "minimum": 1 },
            { "type": "string", "enum": ["custom", "unlimited"] }
          ]
        },
        "redistribution_policy": {
          "type": "string",
          "enum": ["no_redistribution", "limited", "custom_contract"]
        },
        "commercial_use": { "type": "boolean" }
      }
    },
    "price_usd": { "type": "number", "minimum": 0 },
    "currency": { "type": "string", "enum": ["USD"] },
    "billing_type": { "type": "string", "enum": ["one_time", "subscription"] },
    "availability": { "type": "string", "enum": ["active", "archived", "draft"] },
    "created_at": { "type": "string", "format": "date-time" },
    "updated_at": { "type": "string", "format": "date-time" },
    "urls": {
      "type": "object",
      "additionalProperties": false,
      "required": ["product_page", "docs", "download_endpoint", "sample_endpoint", "changelog"],
      "properties": {
        "product_page": { "type": "string", "format": "uri" },
        "docs": { "type": "string", "format": "uri" },
        "download_endpoint": { "type": "string", "format": "uri" },
        "sample_endpoint": { "type": "string", "format": "uri" },
        "changelog": { "type": "string", "format": "uri" }
      }
    },
    "tags": {
      "type": "array",
      "items": { "type": "string" }
    },
    "keywords": {
      "type": "array",
      "items": { "type": "string" }
    },
    "support": {
      "type": "object",
      "additionalProperties": false,
      "required": ["level", "contact", "sla"],
      "properties": {
        "level": { "type": "string", "enum": ["community", "email", "priority"] },
        "contact": { "type": "string" },
        "sla": { "type": "string" }
      }
    },
    "bundle_contents": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["product_id", "quantity", "notes"],
        "properties": {
          "product_id": { "type": "string" },
          "quantity": { "type": "integer", "minimum": 1 },
          "notes": { "type": "string" }
        }
      }
    },
    "execution_mode": {
      "type": "string",
      "enum": ["none", "research", "paper", "live", "mixed"]
    },
    "market_type": {
      "type": "string",
      "enum": ["none", "prediction", "crypto_perps", "hybrid"]
    },
    "connector_type": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["api", "websocket", "webhook"]
      }
    },
    "risk_level": {
      "type": "string",
      "enum": ["low", "med", "high"]
    },
    "risk_profile": {
      "type": "object",
      "additionalProperties": false,
      "required": ["max_position_size", "max_exposure_pct", "stop_rules", "circuit_breaker_rules"],
      "properties": {
        "max_position_size": { "type": "string" },
        "max_exposure_pct": { "type": "number", "minimum": 0, "maximum": 100 },
        "stop_rules": {
          "type": "array",
          "items": { "type": "string" }
        },
        "circuit_breaker_rules": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    },
    "disclosure_required": { "type": "boolean" },
    "disclosure_profile": {
      "type": "object",
      "additionalProperties": false,
      "required": ["disclosure_version", "ack_required", "display_context"],
      "properties": {
        "disclosure_version": { "type": "string" },
        "ack_required": { "type": "boolean" },
        "display_context": {
          "type": "array",
          "items": { "type": "string", "enum": ["product_page", "checkout", "download"] }
        }
      }
    },
    "trade_action_schema": {
      "type": ["object", "string"]
    },
    "market_data_schema": {
      "type": ["object", "string"]
    }
  },
  "allOf": [
    {
      "if": { "properties": { "type": { "const": "bundle" } } },
      "then": {
        "required": ["bundle_contents"]
      }
    },
    {
      "if": {
        "properties": {
          "type": { "enum": ["skill", "agent"] }
        }
      },
      "then": {
        "required": ["input_schema", "output_schema"]
      }
    },
    {
      "if": {
        "properties": {
          "execution_mode": { "const": "live" }
        }
      },
      "then": {
        "required": ["disclosure_required", "disclosure_profile", "risk_profile"],
        "properties": {
          "disclosure_required": { "const": true }
        }
      }
    },
    {
      "if": {
        "properties": {
          "risk_level": { "const": "high" }
        }
      },
      "then": {
        "required": ["disclosure_required"]
      }
    }
  ]
}
